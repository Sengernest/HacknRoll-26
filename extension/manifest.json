{
  "manifest_version": 3,
  "name": "Fate Mode (MVP)",
  "version": "0.0.1",
  "description": "Roll a dice before any click happens.",
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["core.js", "punishments.js", "rewards.js", "fateLogic.js", "content.js"],
      "css": ["overlay.css"],
      "run_at": "document_start"
    }
  ]
}
